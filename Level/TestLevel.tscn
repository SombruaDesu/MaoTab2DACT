[sub_resource type="GDScript" id="GDScript_7x0wu"]
resource_name = "Box"
script/source = "extends Area2D

func _ready() -> void: #当游戏开始时
	var a = $Sprite2D/AnimationPlayer as AnimationAsyncPlayer; # 获取场景里的人物模型
	a.Play(\"Run\",1); 				# 播放Idle动画，一次性
	await a.OnAnimationFinished;	# 再次等待上面触发的动画播放完毕
	print(\"Run ok\")					# 输出日志 跑的动画播完了
	pass

func _on_body_entered(body: Node2D) -> void:
	if body is not Player:
		return
	var p = body as Player;
	p.SetInteractionAction(InteractionAction.Create(0, \"Node_TestDlg\"));
	pass
"

